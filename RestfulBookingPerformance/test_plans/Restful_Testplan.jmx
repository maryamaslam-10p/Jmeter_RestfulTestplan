            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonOutput
import java.time.LocalDate
import java.time.format.DateTimeFormatter
import java.util.concurrent.ThreadLocalRandom


def fname = vars.get(&quot;firstname&quot;) ?: &quot;First_&quot; + UUID.randomUUID().toString()[0..6]
def lname = vars.get(&quot;lastname&quot;) ?: &quot;Last_&quot; + UUID.randomUUID().toString()[0..6]
def totalprice = (vars.get(&quot;totalprice&quot;) ?: &quot;150&quot;) as Integer
def additionalneeds = vars.get(&quot;additionalneeds&quot;) ?: &quot;Breakfast&quot;

// dynamic booking dates: checkin = tomorrow, checkout = +3 days
def fmt = DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd&quot;)

// Random offset between 1 and 7 days
def checkinOffset = ThreadLocalRandom.current().nextInt(1, 8)
def checkoutOffset = checkinOffset + ThreadLocalRandom.current().nextInt(1, 5)

def checkin = LocalDate.now().plusDays(checkinOffset).format(fmt)
def checkout = LocalDate.now().plusDays(checkoutOffset).format(fmt)

vars.put(&quot;checkin&quot;, checkin)
vars.put(&quot;checkout&quot;, checkout)

def booking = [
  firstname: fname,
  lastname: lname,
  totalprice: totalprice,
  depositpaid: true,
  bookingdates: [
    checkin: checkin,
    checkout: checkout
  ],
  additionalneeds: additionalneeds
]

def jsonBody = JsonOutput.toJson(booking)
vars.put(&quot;bookingBody&quot;, jsonBody) // used in boday Data of Create booking request
vars.put(&quot;expectedFirstname&quot;, fname)  // for later assertion in Get Booking by Id
</stringProp>
