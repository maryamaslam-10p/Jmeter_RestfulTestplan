          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion (to validate returned first name equals expected)" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper

def response = prev.getResponseDataAsString()
def code = prev.getResponseCode().toInteger()
if (code != 200) {
    throw new AssertionError(&quot;Expected HTTP 200 but got ${code}&quot;)
}
def json = new JsonSlurper().parseText(response)
def expected = vars.get(&quot;expectedFirstname&quot;)
if (json.firstname != expected) {
    throw new AssertionError(&quot;Firstname mismatch. expected=&apos;${expected}&apos;, actual=&apos;${json.firstname}&apos;&quot;)
}

log.info(&quot;Assertion passed for booking: firstname=${json.firstname}, code=${code}&quot;)
</stringProp>
